{
  "name": "@advanced/component-mosaic-table",
  "version": "1.0.0",
  "description": "Mosaic table component",
  "main": "dist/table.umd.js",
  "module": "src/lib/table.js",
  "browser": "dist/table.umd.js",
  "scripts": {
    "build": "run-s build:validate build:table",
    "build:table": "rollup -c rollup.config.table.js",
    "build:validate": "npm run gen:schema && rollup -c rollup.config.validate.js",
    "dev": "run-p dev:**",
    "dev:build": "rollup -c rollup.config.table.js -w",
    "dev:serve": "live-server",
    "gen:schema": "ajv compile -s src/gen/schema.json -o src/gen/temp_validate.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "ajv-cli": "^3.0.0",
    "live-server": "^1.2.0",
    "npm-run-all": "^4.1.3",
    "rollup": "^0.66.4",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-cjs-es": "^0.7.0",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-uglify": "^6.0.0",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "es6-promise": "^4.2.5",
    "whatwg-fetch": "^3.0.0"
  }
}
