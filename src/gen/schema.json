{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "striped": {
      "type": "boolean"
    },
    "dark": {
      "type": "boolean"
    },
    "bordered": {
      "type": "boolean"
    },
    "hover": {
      "type": "boolean"
    },
    "fixedHeight": {
      "type": "boolean"
    },
    "headers": {
      "type": "boolean"
    },
    "footers": {
      "type": "boolean"
    },
    "displayedEntries": {
      "type": "boolean"
    },
    "schema": {
      "oneOf": [
        {
          "type": "string",
          "format": "uri",
          "pattern": "^https?://"
        },
        {
          "type": "object"
        }
      ]
    },
    "page": {
      "type": "integer"
    },
    "size": {
      "oneOf": [
        {
          "type": "integer",
          "minimum": 10,
          "maximum": 10
        },
        {
          "type": "integer",
          "minimum": 25,
          "maximum": 25
        },
        {
          "type": "integer",
          "minimum": 50,
          "maximum": 50
        }
      ]
    }
  },
  "required": [
    "schema"
  ]
}
