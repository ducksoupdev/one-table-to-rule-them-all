{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "striped": {
      "type": "boolean"
    },
    "dark": {
      "type": "boolean"
    },
    "bordered": {
      "type": "boolean"
    },
    "hover": {
      "type": "boolean"
    },
    "fixedHeight": {
      "type": "boolean"
    },
    "headers": {
      "type": "boolean"
    },
    "headClassName": {
      "type": "string"
    },
    "footers": {
      "type": "boolean"
    },
    "footClassName": {
      "type": "string"
    },
    "displayedEntries": {
      "type": "boolean"
    },
    "schema": {
      "type": "object"
    },
    "pageIndex": {
      "type": "integer",
      "minimum": 0
    },
    "pageSize": {
      "oneOf": [
        {
          "type": "integer",
          "minimum": 10,
          "maximum": 10
        },
        {
          "type": "integer",
          "minimum": 25,
          "maximum": 25
        },
        {
          "type": "integer",
          "minimum": 50,
          "maximum": 50
        }
      ]
    },
    "visiblePageCount": {
      "type": "integer",
      "minimum": 4
    }
  },
  "required": [
    "schema"
  ]
}
